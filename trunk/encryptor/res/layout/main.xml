<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/PassID" android:id="@+id/PassIDText"></TextView>

<Spinner android:layout_height="wrap_content" android:id="@+id/PasswordIDSpinner" android:layout_width="fill_parent"></Spinner><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/AlogTypeText" android:text="@string/algoType"></TextView>
<Spinner android:layout_height="wrap_content" android:id="@+id/AlgoTypeSpinner" android:layout_width="fill_parent"></Spinner>

<Button android:layout_height="wrap_content" android:id="@+id/ContinueButton" android:text="@string/Continue" android:layout_width="fill_parent"></Button>
</LinearLayout>
